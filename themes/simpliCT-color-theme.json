{
	"name": "simpliCT",
	"type": "dark",
	"tokenColors": [
		{
			"name": "comment",
			"scope": ["comment"],
			"settings": { "fontStyle": "italic", "foreground": "#475e68" }
		},
		{
			"name": "Strings",
			"scope": ["string"],
			"settings": { "foreground": "#00cf8e" }
		},
		{
			"name": "text",
			"scope": ["text", "source"],
			"settings": { "foreground": "#d1d1d1" }
		},
		{
			"name": "numbers",
			"scope": ["constant.numeric"],
			"settings": { "foreground": "#f5d66f" }
		},
		{
			"name": "Colors",
			"scope": ["constant.other.color"],
			"settings": { "foreground": "#d1d1d1" }
		},

		// -- -- HTML -- -- //

		{
			"name": "tag",
			"scope": [
				"entity.name.tag",
				"punctuation.definition.tag",
				"punctuation.separator.key-value.html",
				"source.css-ignored-vscode"
			],
			"settings": { "foreground": "#df6f75" }
		},
		{
			"name": "tag",
			"scope": ["meta.tag.metadata.doctype"],
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "attributes",
			"scope": ["entity.other.attribute-name"],
			"settings": { "fontStyle": "italic", "foreground": "#e09316" }
		},

		// -- -- CSS -- -- //

		{
			"name": "property",
			"scope": ["support.type", "punctuation.separator.key-value.css", "punctuation.terminator.rule.css"],
			"settings": { "foreground": "#00b7e5" }
		},
		{
			"name": "value",
			"scope": ["meta.property-value"],
			"settings": { "foreground": "#6ff576" }
		},
		{
			"name": "variable",
			"scope": ["variable.css", "variable.argument.css"],
			"settings": { "fontStyle": "italic", "foreground": "#ff79c6" }
		},
		{
			"name": "punctuation",
			"scope": ["punctuation.separator.list.comma", "punctuation.section.function"],
			"settings": { "foreground": "#d1d1d1" }
		},
		{
			"name": "media queries",
			"scope": ["meta.at-rule", "keyword.control.at-rule"],
			"settings": { "foreground": "#f5d66f" }
		},

		// -- -- JS -- -- //

		{
			"name": "boolean",
			"scope": ["constant.language.boolean", "constant.language.json"],
			"settings": { "foreground": "#df6f75" }
		},
		{
			"name": "null and undefined",
			"scope": ["constant.language.null", "constant.language.undefined"],
			"settings": { "foreground": "#df6f75" }
		},
		{
			"name": "template literals",
			"scope": ["punctuation.definition.template-expression"],
			"settings": { "foreground": "#e09316" }
		},
		{
			"name": "escape char",
			"scope": ["constant.character.escape"],
			"settings": { "foreground": "#6ff576" }
		},
		{
			"name": "storage",
			"scope": ["storage"],
			"settings": { "foreground": "#e09316" }
		},
		// {
		// 	"name": "storage modifier",
		// 	"scope": ["storage.modifier"],
		// 	"settings": { "fontStyle": "italic" }
		// },
		{
			"name": "control flow",
			"scope": ["keyword.control.flow"],
			"settings": { "foreground": "#aa7ee1" }
		},
		{
			"name": "variable name",
			"scope": ["meta.definition.variable", "variable.other.readwrite", "variable.other.object"],
			"settings": { "foreground": "#00b7e5" }
		},
		{
			"name": "variable object",
			"scope": ["variable.other.constant.object"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "object key",
			"scope": ["meta.object-literal"],
			"settings": { "foreground": "#aa7ee1" }
		},
		{
			"name": "function name",
			"scope": ["entity.name.function", "punctuation.accessor", "entity.other.inherited-class"],
			"settings": { "foreground": "#f5d66f" }
		},
		{
			"name": "function parameter",
			"scope": ["variable.parameter"],
			"settings": { "foreground": "#00b7e5" }
		},
		{
			"name": "other property",
			"scope": ["variable.other.property", "support.variable.property"],
			"settings": { "foreground": "#f5d66f" }
		},
		{
			"name": "support functions",
			"scope": ["support.function"],
			"settings": { "foreground": "#f5d66f" }
		},
		{
			"name": "Class",
			"scope": [
				"entity.name.type.class",
				"support.class.builtin",
				"entity.name.type",
				"support.constant.math"
			],
			"settings": { "foreground": "#aa7ee1" }
		},
		{
			"name": "Error",
			"scope": ["support.class.error"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "new",
			"scope": ["keyword.operator.new"],
			"settings": { "foreground": "#e09316" }
		},
		{
			"name": "conditionals",
			"scope": ["keyword.control.conditional", "keyword.operator.ternary"],
			"settings": { "foreground": "#df6f75" }
		},
		{
			"name": "loops",
			"scope": ["keyword.control.loop"],
			"settings": { "foreground": "#aa7ee1" }
		},
		{
			"name": "punctuation",
			"scope": ["punctuation.terminator.statement", "keyword.operator"],
			"settings": { "foreground": "#d1d1d1" }
		},
		{
			"name": "operators",
			"scope": ["keyword.operator.logical"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "spread and rest",
			"scope": ["keyword.operator.spread", "keyword.operator.rest"],
			"settings": { "foreground": "#00b7e5" }
		},
		{
			"name": "comparison and contrast",
			"scope": ["keyword.operator.comparison", "keyword.operator.relational"],
			"settings": { "foreground": "#df6f75" }
		},
		{
			"name": "keyword control",
			"scope": ["keyword.control"],
			"settings": { "foreground": "#aa7ee1" }
		},
		{
			"name": "this",
			"scope": ["variable.language.this"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "super",
			"scope": ["variable.language.super"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "Promise",
			"scope": ["support.class.promise"],
			"settings": { "foreground": "#ff79c6" }
		},
		{
			"name": "modifier",
			"scope": ["modifier"],
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "markup quote",
			"scope": ["markup.quote"],
			"settings": { "fontStyle": "italic" }
		},
		{
			"name": "entity tag",
			"scope": ["tag.decorator.js entity.name.tag.js", "tag.decorator.js punctuation.definition.tag.js"],
			"settings": { "fontStyle": "italic" }
		},

		// Untouched

		{
			"name": "Invalid",
			"scope": ["invalid", "invalid.illegal"],
			"settings": { "foreground": "#FF5370" }
		},
		{
			"name": "Inserted",
			"scope": ["markup.inserted"],
			"settings": { "foreground": "#f5d66f" }
		},
		{
			"name": "Deleted",
			"scope": ["markup.deleted"],
			"settings": { "foreground": "#FF5370" }
		},
		{
			"name": "Changed",
			"scope": ["markup.changed"],
			"settings": { "foreground": "#aa7ee1" }
		},
